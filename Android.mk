# It is necessory to tell makefile about the current path
LOCAL_PATH := $(call my-dir)

# Shared libraries
include $(CLEAR_VARS)
LOCAL_MODULE := libigc
LOCAL_PROPRIETARY_MODULE := true
LOCAL_MODULE_CLASS := SHARED_LIBRARIES
LOCAL_MODULE_TAGS := optional
LOCAL_MODULE_STEM := libigc
LOCAL_MODULE_SUFFIX := $(TARGET_SHLIB_SUFFIX)
LOCAL_MODULE_OWNER := intel
LOCAL_MULTILIB := both
LOCAL_SRC_FILES_x86 := lib/x86/libigc$(LOCAL_MODULE_SUFFIX)
LOCAL_SRC_FILES_x86_64 := lib/x86_64/libigc$(LOCAL_MODULE_SUFFIX)
LOCAL_MODULE_PATH_32 := $(TARGET_OUT_VENDOR)/lib/
LOCAL_MODULE_PATH_64 := $(TARGET_OUT_VENDOR)/lib64/
LOCAL_SHARED_LIBRARIES := libc liblog libz
OVERRIDE_BUILT_MODULE_PATH := $(TARGET_OUT_INTERMEDIATE_LIBRARIES)
LOCAL_STRIP_MODULE := true
include $(BUILD_PREBUILT)


